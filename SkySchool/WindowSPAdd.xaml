<Window x:Class="SkySchool.WindowSPAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkySchool"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="Закрепление дисциплины" Height="241" Width="258" Background="AliceBlue" Icon="Resources/Sky_ico.ico">
    <Grid Background="Aqua" Width="250" Height="210">
        <ComboBox Text="{Binding Disciplina.Nazvanie}" x:Name="ComboBoxD" Width="230" Height="60" DisplayMemberPath="Nazvanie" Margin="10,10,10,140"/>
        <ComboBox Text="{Binding Tip_Zanyatiya.Tip}" x:Name="ComboBoxT" Width="230" Height="60" DisplayMemberPath="Tip" Margin="10,75"/>
        <Button Content="Сохранить" Name="BtnSave" Click="BtnSave_Click" Margin="10,140,10,10"/>
    </Grid>
</Window>
